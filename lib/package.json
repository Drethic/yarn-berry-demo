{
  "name": "yarn-error-lib",
  "version": "1.0.0",
  "description": "library for yarn error",
  "scripts": {
    "clean:dist": "yarn rimraf dist",
    "make:dist": "yarn mkdirp dist",
    "make:css": "node-sass src/sass/index.scss dist/index.css",
    "cleanMake": "yarn clean:dist && yarn make:dist",
    "copy:font": "cp -r src/sass/fonts dist",
    "copy:img": "cp -r src/sass/img dist",
    "copy": "yarn copy:font && yarn copy:img",
    "build": "yarn cleanMake && yarn make:css && yarn copy && microbundle --jsx React.createElement --jsxFragment React.Fragment -f es,cjs,modern",
    "dev": "yarn cleanMake && yarn make:css && yarn copy && microbundle watch --jsx React.createElement --jsxFragment React.Fragment -f es,cjs,modern --no-compress",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage=true"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.4",
    "chartjs-plugin-annotation": "^0.5.7",
    "date-fns": "^2.16.1",
    "dayjs": "^1.9.8",
    "formik": "^2.2.6",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "raw.macro": "^0.4.2",
    "react": "^16.14.0",
    "react-chartjs-2": "^2.11.1",
    "react-datepicker": "^3.4.1",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-grid-layout": "^1.2.0",
    "react-markdown": "^5.0.3",
    "react-resizable": "^1.11.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "reactstrap": "^8.9.0",
    "remark-gfm": "^1.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^5.0.0",
    "react-scripts": "^3.4.3",
    "rimraf": "^3.0.2",
    "sass": "^1.32.0"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js"
}
